<h1>Users</h1>

<ul>
  <% @users.each do |user| %>
    <li><%= link_to user.name, stories_path(user) %></li>
    <li>
      <%= link_to "Edit", edit_user_path(user) %> |
      <%= link_to "Delete", user_path(user),
      :data => {:confirm => "Are you sure?",
        :method => "delete"} %>
    </li>
  <% end %>
</ul>

<%= link_to "Add New User", new_user_path %>
